{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@constants": ["src/constants/constants"],
      "@types": ["src/constants/types"],
      "@utils": ["src/constants/utils"],
      "@cards/*": ["src/components/cards/*"]
    }
  }
}
